  "module": "./lib/index.js",
  "author": "iappx",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "rimraf": "^3.0.2",
    "jest": "^29.2.1",
    "@types/jest": "^29.5.3",
    "@types/superagent": "^4.1.15",
    "eslint": "^8.6.0",
    "superagent": "^8.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "axios": "1.11.0"
  }
