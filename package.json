{
  "name": "@iappx/entity-repo",
  "version": "3.0.0",
  "description": "A TypeScript library that abstracts data sources and provides a unified context-based access to entities, queries, and transports.",
  "private": false,
  "homepage": "https://github.com/iappx/entity-repo#readme",
  "keywords": [
    "typescript",
    "entity",
    "repository",
    "orm",
    "data-source",
    "data-access",
    "context",
    "transport",
    "query-builder",
    "abstraction",
    "rest",
    "axios",
    "framework"
  ],
  "scripts": {
    "build": "rimraf ./lib && tsc --build tsconfig.json",
